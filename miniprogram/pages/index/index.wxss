
.custom_card {
  flex: 1;
  display: flex;
  flex-direction: column;
  padding-top: 20rpx;
}
.custom_self {
  display: flex;
  align-items: center;
  justify-content: center;
}
.custom_button {
  width: 660rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border: 1px solid #ccc;
  font-size: 30rpx;
  margin-bottom: 20rpx;
  border-radius: 8rpx;
}
